<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.addStoreController">
   <children>
      <Label layoutX="65.0" layoutY="44.0" text="Store Name:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="71.0" text="Store Opening Hours:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="100.0" text="Store Closing Hours:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="sName" layoutX="151.0" layoutY="40.0" prefHeight="25.0" prefWidth="174.0" />
      <TextField fx:id="sOHours" layoutX="151.0" layoutY="67.0" prefHeight="25.0" prefWidth="174.0" />
      <TextField fx:id="sCHours" layoutX="151.0" layoutY="96.0" prefHeight="25.0" prefWidth="174.0" />
      <Button fx:id="sAddStore" layoutX="493.0" layoutY="40.0" mnemonicParsing="false" onAction="#addStoreButton" text="ADD STORE" />
      <Button fx:id="sBack" layoutX="545.0" layoutY="361.0" mnemonicParsing="false" onAction="#backButton" text="Back" />
      <Label fx:id="sMsg" layoutX="224.0" layoutY="192.0" text="Label" />
      <Label fx:id="sUser" layoutX="22.0" layoutY="23.0" text="Label" />
   </children>
</AnchorPane>
