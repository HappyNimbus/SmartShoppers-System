<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.removeItemAdminController">
   <children>
      <Label layoutX="27.0" layoutY="50.0" text="Select store to remove Item:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <ChoiceBox fx:id="adRStore" layoutX="195.0" layoutY="46.0" prefHeight="25.0" prefWidth="211.0" />
      <Label layoutX="27.0" layoutY="108.0" text="Item name to remove:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="adRName" layoutX="164.0" layoutY="104.0" prefHeight="25.0" prefWidth="241.0" />
      <Button fx:id="adRemove" layoutX="456.0" layoutY="104.0" mnemonicParsing="false" onAction="#removeLoginButton" prefHeight="25.0" prefWidth="107.0" text="Remove" />
      <Button fx:id="adRBack" layoutX="528.0" layoutY="361.0" mnemonicParsing="false" onAction="#backLoginButton" text="Back" />
      <Label fx:id="adRUser" layoutX="27.0" layoutY="14.0" text="Label" />
      <Label fx:id="adRMsg" layoutX="286.0" layoutY="192.0" text="Label" />
   </children>
</AnchorPane>
