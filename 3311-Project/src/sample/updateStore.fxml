<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.updateStoreController">
   <children>
      <Label layoutX="14.0" layoutY="35.0" text="Opening Hours:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="21.0" layoutY="64.0" text="Closing Hours:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="92.0" text="Store Avalibility:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="uOpen" layoutX="121.0" layoutY="31.0" promptText="Opening Hours" />
      <TextField fx:id="uClose" layoutX="121.0" layoutY="60.0" promptText="Closing Hours" />
      <Button fx:id="uOHours" layoutX="286.0" layoutY="31.0" mnemonicParsing="false" onAction="#updateOpenB" text="Update Opening Hours" />
      <Button fx:id="uCHours" layoutX="286.0" layoutY="60.0" mnemonicParsing="false" onAction="#updateCloseB" prefHeight="25.0" prefWidth="139.0" text="Update Closing Hours" />
      <Button fx:id="uAvab" layoutX="286.0" layoutY="88.0" mnemonicParsing="false" onAction="#updateAvaB" prefHeight="25.0" prefWidth="139.0" text="Update Avalibility" />
      <RadioButton fx:id="uT" layoutX="121.0" layoutY="92.0" mnemonicParsing="false" text="TRUE">
         <toggleGroup>
            <ToggleGroup fx:id="uAva" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="uF" layoutX="183.0" layoutY="92.0" mnemonicParsing="false" text="FALSE" toggleGroup="$uAva" />
      <Button fx:id="uBack" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#backB" text="Back" />
      <Label fx:id="uMsg" layoutX="169.0" layoutY="160.0" text="Label" />
      <Label fx:id="uUser" layoutX="506.0" layoutY="35.0" text="Label" />
      <Label fx:id="uStore" layoutX="506.0" layoutY="56.0" text="Label" />
   </children>
</AnchorPane>
